>OPEN KEYBOARD SHORTCUTS JSON

// Place your key bindings in this file to override the defaults
// cmd+D to duplicate lines 
// ctrl+tab to move tab foward like chrome
// ctrl+tab to move tab back like chrome
// ctrl+up jump +N lines up [customizable] THESE TWO HAVE TO REMOVE MAC SHORTCUT FOR ctrl+up on settings
// ctrl+up jump +N lines down [customizable] THESE TWO HAVE TO REMOVE MAC SHORTCUT FOR ctrl+down on settings

[
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },

    {
        "key": "cmd+d",
        "command": "editor.action.copyLinesDownAction",
    },

    //JUMP MORE LINES

    {
        "key": "ctrl+up",
        "command": "cursorMove",
        "args": {
            "to": "up",
            "by": "line",
            "value": 10
        },
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+down",
        "command": "cursorMove",
        "args": {
            "to": "down",
            "by": "line",
            "value": 10
        },
        "when": "editorTextFocus"
    },

]
